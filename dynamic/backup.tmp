  <script>
    async function detectNewestImageURL(max) {
      for (let i = 1; i <= max; i++) {
        const url = `https://www.sellinseconds.in/dynamic/images/${i}.webp?_=${Date.now()}`;
        try {
          const res = await fetch(url, { method: 'HEAD' });
          if (!res.ok) return `https://www.sellinseconds.in/dynamic/images/${i - 1}.webp`;
        } catch {
          return `https://www.sellinseconds.in/dynamic/images/${i - 1}.webp`;
        }
      }
      return `https://www.sellinseconds.in/dynamic/images/${max}.webp`;
    }

    (async function updateOGThumbnail() {
      const newestURL = await detectNewestImageURL(30);
      const ogImage = document.querySelector('meta[property="og:image"]');
      const twitterImage = document.querySelector('meta[name="twitter:image"]');
      if (ogImage) ogImage.setAttribute('content', newestURL);
      if (twitterImage) twitterImage.setAttribute('content', newestURL);
    })();
  </script>
</head>
<body>
  <div class="container">
    <header>
      <div class="marquee-container">
        <div class="marquee-track">
          <img class="marquee-image" src="https://www.sellinseconds.in/sellhead.webp" alt="SellInSeconds Header Banner">
        </div>
      </div>
    </header>
    <div class="gallery-grid"></div>
  </div>

  <a class="whatsapp-float" href="https://wa.me/919886566379" target="_blank" title="Chat on WhatsApp">
    <i class="fab fa-whatsapp"></i>
  </a>

  <button id="scrollToTopBtn" onclick="window.scrollTo({ top: 0, behavior: 'smooth' });">
    <i class="fas fa-arrow-up"></i>
  </button>

  <script>
    window.addEventListener('scroll', () => {
      document.getElementById('scrollToTopBtn').style.display = window.scrollY > 300 ? 'block' : 'none';
    });

    async function detectNewestImage(max) {
      for (let i = 1; i <= max; i++) {
        const url = `https://www.sellinseconds.in/dynamic/images/${i}.webp?_=${Date.now()}`;
        try {
          const res = await fetch(url, { method: 'HEAD' });
          if (!res.ok) return i - 1;
        } catch {
          return i - 1;
        }
      }
      return max;
    }

    document.addEventListener("DOMContentLoaded", async () => {
      const maxImages = 30;
      const gallery = document.querySelector(".gallery-grid");
      const newestImageNumber = await detectNewestImage(maxImages);

      const orderedImages = [];
      for (let i = newestImageNumber; i >= 1; i--) orderedImages.push(i);
      for (let i = maxImages; i > newestImageNumber; i--) orderedImages.push(i);

      gallery.innerHTML = orderedImages.map((num, index) => 
        `<div class="card-wrapper ${index === 0 ? 'top-most' : ''}" style="animation: fadeIn 0.5s forwards ${index * 0.1}s">
          <div class="card">
            <div class="card-img-container">
              <img src="https://www.sellinseconds.in/dynamic/images/${num}.webp" alt="Product ${num}" loading="lazy"
                   onerror="this.closest('.card-wrapper').style.display='none';">
            </div>
          </div>
          <button class="whatsapp-card-btn" data-product-img-name="${num}">
            <i class="fab fa-whatsapp"></i> I am interested in this device
          </button>
        </div>`
      ).join('');

      gallery.addEventListener('click', function (e) {
        const button = e.target.closest(".whatsapp-card-btn");
        if (button) {
          const productImageName = button.getAttribute('data-product-img-name');
          const message = encodeURIComponent(`I am interested in this device. https://www.sellinseconds.in/dynamic/wacust/${productImageName}.html`);
          window.open(`https://wa.me/919886566379?text=${message}`, '_blank');
        }
      });
    });
  </script>
</body>
